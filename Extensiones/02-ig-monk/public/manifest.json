{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [{
        "matches":
        [
            "https://instagram.com/*"
        ],
        "js": ["content.js"]
        
    }],
    "action":
    {
        "default_popup": "popup.html"
    },
    "permissions":
    [
        "storage"
    ]
  }